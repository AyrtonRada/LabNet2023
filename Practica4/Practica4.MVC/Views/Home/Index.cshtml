@model List<Practica4.Entities.Employees>
@{
    ViewBag.Title = "Lista de Empleados";
}

<h1>Bienvenido a la Lista de Empleados</h1>

<table class="table">
    <tr>
        <td>ID</td>
        <td>NOMBRE</td>
        <td>APELLIDO</td>
    </tr>

    @foreach (var empleado in Model)
    {
    <tr>
        <td>@empleado.EmployeeID</td>
        <td>@empleado.FirstName</td>
        <td>@empleado.LastName</td>
    </tr>
    }
</table>
